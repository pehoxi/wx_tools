.wrapper{
  position: relative;
  background-color: #eee5e5;
}
.lj_top{
  width: 100%;
  height: 150px;
  background-color: #eee5e5;
}
.lj_top>image{
  height: 150px;
  margin-left: 25px;
}
.ljfl{
  border: 1px solid #999999;
  width: 200px;
  height: 20px;
  position: absolute;
  left: 85px;
  top: 65px;
  background-color: white;
}
.lj_center{
  background-color:  #eee5e5;
  width: 100%;
  height: 350px;
  padding: 5px;
  padding-top: 15px;
  padding-left: 20px;
}
.btn{
  width: 80px;
  height: 30px;
  font-size: 12px;
  float: left;
  margin-left: 15px;
  margin-top: 15px;
}

.lj_cen_hh{
  position: absolute;
  left: 0px;
  top: 150px;
}
.lj_bottom{
  width: 100%;
  height: 80px;
  background-color: white;
  /* border: 1px solid red; */
}
.lj_bottom view{
  height: 80px;
  width: 80px;
  border-radius: 5px;
  float: left;
  margin-left: 10px;
}
.lj_bot_one{
  background-color: red;
}
.lj_bot_two{
  background-color: #1c1858;
}
.lj_bot_three{
  background-color:rgba(44, 21, 21, 0.714);
}
.lj_bot_four{
  background-color:black;
}
.lj_bottom view image{
  height: 80px;
  width: 80px;
}
.ljjs{
  width: 250px;
  height: 250px;
  position: absolute;
  left: 60px;
  top: 150px;
  border-radius: 25px;
  padding: 10px;
  z-index: 100;
  color: white;
  font-weight: bolder;
}
.ljjs image{
  width: 100px;
  height: 100px;
  padding-left: 80px;
}
.ljjs_one{
  background-color: red;
}
.ljjs_two{
  background-color: #1c1858;
}
.ljjs_three{
  background-color:rgba(44, 21, 21, 0.714);
}
.ljjs_four{
  background-color:black;
}