view{
  margin: 0 auto;
  background-color: #e9e9e9;
  width: 700rpx;
   display: flex;
   /* position: relative; */
    flex-wrap: wrap;
    align-content: flex-start;
}

.w_img{
  padding-left: 20px;
  height: 100px;
   width: 25%;
   /* border: 1px solid red; */
   box-sizing: border-box;
   display: flex;
   flex-direction: column;
    /* 内容垂直居中 */
   align-items: center;
      /* 内容水平居中 */
   justify-content: center;
}
.w_img image{
  height: 40px;
  width: 40px;
}
.w_img:nth-of-type(2){
  padding-left: 10px;
}
